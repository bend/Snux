package {	import flash.display.MovieClip;	public class ContainerObstacle extends MovieClip {		private var obstacle:MovieClip;		public function ContainerObstacle() {		}		public function addContainerObstacleH():MovieClip {			obstacle= new ObstacleH();			this.addChild(obstacle);			return this;		}		public function addContainerObstacleV():MovieClip {			obstacle= new ObstacleV();			this.addChild(obstacle);			return this;		}		public function testSerpentHitObstacle(vectSerpent:Array):Boolean {			return this.hitTestPoint(vectSerpent[0].x,vectSerpent[0].y,false);		}	}//fin class}//fin package